{
  "name": "whatsmenu-desktop",
  "productName": "WhatsMenu Desktop",
  "version": "0.0.16",
  "description": "Vers√£o Desktop para WhatsMenu",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make --arch=\"ia32,x64\"",
    "publish": "electron-forge publish ",
    "publish:32bits": "electron-forge publish --arch=\"ia32\"",
    "publish:64bits": "electron-forge publish --arch=\"x64\"",
    "release": "npm version patch -m \"Upgrade version \" && npm run publish:32bits && npm run publish:64bits",
    "release:no-make": "npm version patch -m \"Upgrade version \" && electron-forge publish --from-dry-run",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.3.1",
    "@electron-forge/maker-deb": "^7.3.1",
    "@electron-forge/maker-rpm": "^7.3.1",
    "@electron-forge/maker-squirrel": "^7.3.1",
    "@electron-forge/maker-zip": "^7.3.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.3.1",
    "@electron-forge/plugin-fuses": "^7.2.0",
    "@electron-forge/plugin-vite": "^7.3.1",
    "@electron-forge/publisher-github": "^7.4.0",
    "@electron/fuses": "^1.7.0",
    "@types/electron-prompt": "^1.6.5",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.19",
    "electron": "^30.0.1",
    "electron-is-dev": "^3.0.1",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4",
    "vite": "^5.0.12"
  },
  "keywords": [],
  "author": {
    "name": "Grove Company",
    "email": "atendimento@whatsmenu.com.br"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "electron-prompt": "^1.7.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.2.0",
    "qrcode-terminal": "^0.12.0",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "whatsapp-web.js": "github:pedroslopez/whatsapp-web.js#webpack-exodus"
  }
}
